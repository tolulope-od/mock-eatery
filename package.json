{
  "name": "mock-eatery",
  "version": "1.0.0",
  "description": "A mock recipe lookup api",
  "main": "index.js",
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/__mocks__/"
    ]
  },
  "scripts": {
    "prestart": "npm run build",
    "start": "node ./build/index.js",
    "build": "npm run clean && npm run build-babel",
    "build-babel": "babel -d ./build index.js && babel ./src --out-dir build/src",
    "clean": "rm -rf build && mkdir build",
    "dev": "NODE_ENV=dev DEBUG=dev nodemon --exec babel-node .",
    "seed:db": "babel-node ./src/db/migrations.js",
    "pretest": "NODE_ENV=test npm run seed:db",
    "test": "NODE_ENV=test DEBUG=test jest --detectOpenHandles"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tolulope-od/mock-eatery.git"
  },
  "keywords": [
    "api",
    "eatery",
    "recipes"
  ],
  "author": "Tolulope Odueke",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tolulope-od/mock-eatery/issues"
  },
  "homepage": "https://github.com/tolulope-od/mock-eatery#readme",
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/node": "^7.6.3",
    "@babel/preset-env": "^7.6.3",
    "eslint": "^6.5.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "jest": "^24.9.0",
    "nodemon": "^1.19.4",
    "prettier-eslint": "^9.0.0",
    "prettier-eslint-cli": "^5.0.0",
    "supertest": "^4.0.2"
  },
  "dependencies": {
    "@babel/polyfill": "^7.6.0",
    "@hapi/joi": "^15.0.3",
    "bcryptjs": "^2.4.3",
    "connect-redis": "^4.0.3",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "faker": "^4.1.0",
    "gravatar": "^1.8.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "mongoose": "^5.7.5",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "redis": "^2.8.0",
    "uuid": "^3.3.3"
  }
}
